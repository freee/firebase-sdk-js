    "ts-jest": "24.3.0",
    "ts-loader": "^9.3.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "3.0.0"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  }
